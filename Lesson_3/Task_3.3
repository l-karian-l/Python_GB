def my_func(num_1, num_2, num_3):
    if num_1 == num_2 and num_2 == num_3:
        return num_1 + num_2
    sum_n = 0
    array = [num_1, num_2, num_3]
    min_arg = min(array[0], array[1], array[2])
    for el in array:
        if el > min_arg:
            sum_n += el
    return sum_n

